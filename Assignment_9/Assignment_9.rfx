#version 120

#define TEXTURE_SIZE	1024

uniform sampler2D uTexture;

varying vec2 vTexCoord;

const vec2 INV_TEX_SIZE = vec2(1.0 / TEXTURE_SIZE);

vec4[9] make_kernel(vec2 coord) {
	float w = INV_TEX_SIZE.x;
	float h = INV_TEX_SIZE.y;
	
	vec4 n[9];
	
	n[0] = texture2D(uTexture, coord + vec2(-w, -h));
	n[1] = texture2D(uTexture, coord + vec2( 0, -h));
	n[2] = texture2D(uTexture, coord + vec2( w, -h));
	n[3] = texture2D(uTexture, coord + vec2(-w,  0));
	n[4] = texture2D(uTexture, coord + vec2( 0,  0));
	n[5] = texture2D(uTexture, coord + vec2( w,  0));
	n[6] = texture2D(uTexture, coord + vec2(-w,  h));
	n[7] = texture2D(uTexture, coord + vec2( 0,  h));
	n[8] = texture2D(uTexture, coord + vec2( w,  h));
	
	return n;
}

void main() {
	vec4 n[9] = make_kernel(vTexCoord);
	
	vec4 sobel_edge_h = n[2] + (2.0 * n[5]) + n[8] - (n[0] + (2.0 * n[3]) + n[6]);
  	vec4 sobel_edge_v = n[0] + (2.0 * n[1]) + n[2] - (n[6] + (2.0 * n[7]) + n[8]);
	vec4 sobel = sqrt((sobel_edge_h * sobel_edge_h) + (sobel_edge_v * sobel_edge_v));
	
	gl_FragColor = sobel;
}]]></RmShaderCode></RmGLShader>
