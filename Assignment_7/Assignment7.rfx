    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 120

uniform float fTime0_X;
uniform sampler2D Pass2;

uniform vec2  noiseSpeed;
uniform float noiseFrequency;
uniform float noiseIntensity;

uniform float scanLineSize;
uniform float scanLineSpeed;

uniform sampler2D noiseTexture;

varying vec2 texCoord;

const vec3 LUMINANCE_WEIGHTS = vec3(0.299, 0.587, 0.114);

float getLuminance(vec3 color) {
	return dot(color, LUMINANCE_WEIGHTS);
}

void scanLines(inout vec3 color, float size, float speed) {
	color *= abs(sin((texCoord.y * size) + (fTime0_X * speed)));
}

void noise(inout vec3 color, float freq, float intensity, vec2 speed) {
	vec2 vv = texCoord + (speed * fTime0_X);
	color = mix(
		texture2D(noiseTexture, vv * freq).rgb, color,
		clamp(getLuminance(color) + intensity, 0, 1)
	);
}

void main() {
	vec3 pixelColor = texture2D(Pass2, texCoord).rgb;
	
	scanLines(pixelColor, scanLineSize, scanLineSpeed);
	noise(pixelColor, noiseFrequency, noiseIntensity, noiseSpeed);
	
	gl_FragColor = vec4(pixelColor, 1.0);
}]]></RmShaderCode></RmGLShader>
