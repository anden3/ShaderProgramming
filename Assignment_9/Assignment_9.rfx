    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 120

#define ITERATIONS 		12
#define TEXTURE_SIZE	1024

uniform vec2 uInvViewportDims;
uniform sampler2D uTexture;
uniform sampler2D uEdgeTex;

varying vec2 vTexCoord;

const vec3 LUM_WEIGHTS  = vec3(0.299, 0.587, 0.114);
const vec2 INV_TEX_SIZE = vec2(1.0 / TEXTURE_SIZE);

float QUALITY[ITERATIONS];

// Set the amount of pixels to step for every iteration.
void defineQualities() {
	 QUALITY[ 0] = 1;
	 QUALITY[ 1] = 1;
	 QUALITY[ 2] = 1;
	 QUALITY[ 3] = 1;
	 QUALITY[ 4] = 1;
	 QUALITY[ 5] = 1.5;
	 QUALITY[ 6] = 2;
	 QUALITY[ 7] = 2;
	 QUALITY[ 8] = 2;
	 QUALITY[ 9] = 2;
	 QUALITY[10] = 4;
	 QUALITY[11] = 8;
}

float rgb2luma(vec3 rgb) {
	return sqrt(dot(rgb, LUM_WEIGHTS));
}

float textureOffsetLuma(vec2 offset = vec2(0)) {
	return rgb2luma(texture2D(uTexture, vTexCoord + offset * INV_TEX_SIZE).rgb);
}

bool onEdge(vec2 uv) {
	return (texture2D(uEdgeTex, uv).rgb != vec3(0));
}

vec3 fxaa(vec3 colorCenter) {
	// Use edge texture to check if we are on an edge or not.
	if (!onEdge(vTexCoord)) {
		return colorCenter;
	}
	
	
	/* FIND GRADIENT AND EDGE DIRECTION */

	// Get luminosity in current pixel.
	float lumaCenter = rgb2luma(colorCenter);
	
	// Get lumas of direct neighbors.
	float lumaL = textureOffsetLuma(vec2(-1,  0));
	float lumaR = textureOffsetLuma(vec2( 1,  0));
	float lumaD = textureOffsetLuma(vec2( 0, -1));
	float lumaU = textureOffsetLuma(vec2( 0,  1));
	
	// Get lumas of corner neighbors.
	float lumaDL = textureOffsetLuma(vec2(-1, -1));
	float lumaUL = textureOffsetLuma(vec2(-1,  1));
	float lumaDR = textureOffsetLuma(vec2( 1, -1));
	float lumaUR = textureOffsetLuma(vec2( 1,  1));
	
	// Combine lumas.
	float lumaDU 	   = lumaD  + lumaU;
	float lumaLR 	   = lumaL  + lumaR;
	float lumaCornersL = lumaDL + lumaUL;
	float lumaCornersD = lumaDL + lumaDR;
	float lumaCornersR = lumaDR + lumaUR;
	float lumaCornersU = lumaUR + lumaUL;
	
	// Get range of lumas.
	float lumaMin = min(lumaCenter, min(min(lumaD, lumaU), min(lumaL, lumaR)));
	float lumaMax = max(lumaCenter, max(max(lumaD, lumaU), max(lumaL, lumaR)));
	float lumaRange = lumaMax - lumaMin;
	
	// Calculate luma difference along X and Y-axis.
	float deltaX = abs(-2 * lumaL + lumaCornersL) + abs(-2 * lumaCenter + lumaDU) * 2 + abs(-2 * lumaR + lumaCornersR);
	float deltaY = abs(-2 * lumaU + lumaCornersU) + abs(-2 * lumaCenter + lumaLR) * 2 + abs(-2 * lumaD + lumaCornersD);
	
	// Check if edge is horizontal or vertical by checking which difference is greatest.
	// Horizontal = 1, Vertical = 0.
	int edgeDir = int(deltaX >= deltaY);
	
	
	/* GUESS DIRECTION TO CLOSEST BORDER */
	
	// Get lumas opposite to the edge.
	float L1 = vec2(lumaL, lumaD)[edgeDir];
	float L2 = vec2(lumaR, lumaU)[edgeDir];
	
	// Get gradients in this direction.
	float G1 = L1 - lumaCenter;
	float G2 = L2 - lumaCenter;
	
	float GScaled = 0.25 * max(abs(G1), abs(G2));
	
	// Get steepest direction,
	// i.e. the direction to the closest border of the edge.
	int steepestDir = int(abs(G1) >= abs(G2));
	float stepLength = INV_TEX_SIZE[edgeDir];
	
	// Swap direction of steps if going the other way.
	stepLength -= 2 * steepestDir * stepLength;
	
	// Get the average luma in that direction.
	float lumaLocalAverage = 0.5 * (vec2(L2, L1)[steepestDir] + lumaCenter);
	
	// Shift coords by half a pixel in that direction.
	vec2 uvCurrent = vTexCoord;
	uvCurrent[edgeDir] += stepLength * 0,5;
	
	
	/* FIND EDGE BORDERS */
	
	// Set offset to either (INV.x, 0) or (0, INV.y).
	vec2 offset = vec2(0);
	offset[1 - edgeDir] = INV_TEX_SIZE[1 - edgeDir];
	
	// Get coordinates and lumas for either side of the edge.
	vec2 uv1 = uvCurrent - offset;
	vec2 uv2 = uvCurrent + offset;
	
	bvec2 reachedSides = bvec2(false);
	vec2 endLumas = vec2(0);
	
	defineQualities();

	// Continue iterating until both borders of the edge have been found,
	// or max iterations have been reached.
	for (int i = 0; i < ITERATIONS; i++) {
		if (!reachedSides[0]) {
			endLumas[0] = rgb2luma(texture2D(uTexture, uv1).rgb) - lumaLocalAverage;
		}
		if (!reachedSides[1]) {
			endLumas[1] = rgb2luma(texture2D(uTexture, uv2).rgb) - lumaLocalAverage;
		}
		
		reachedSides = greaterThanEqual(abs(endLumas), vec2(GScaled));
		
		if (all(reachedSides)) {
			break;
		}
		
		if (!reachedSides[0]) {
			uv1 -= offset * QUALITY[i];
		}
		if (!reachedSides[1]) {
			uv2 += offset * QUALITY[i];
		}
	}
	
	
	/* ESTIMATE OFFSET */
	
	// Get the distances to the edge borders.
	float dist1 = vTexCoord[1 - edgeDir] - uv1[1 - edgeDir];
	float dist2 = uv2[1 - edgeDir] - vTexCoord[1 - edgeDir];
	
	// Get closest border.
	int smallestDir = int(dist1 > dist2);
	float distFinal = min(dist1, dist2);
	float edgeThickness = dist1 + dist2;
	
	// Get a UV offset in the direction of the closest border.
	float pixelOffset = -distFinal / edgeThickness + 0.5;
	
	// If the luma at center is smaller than at its neighbour,
	// the luma at the end should be larger (same variation).
	bool isLumaCenterSmaller = lumaCenter < lumaLocalAverage;
	bool correctVariation = (endLumas[1 - int(smallestDir)] < 0.0) != isLumaCenterSmaller;
	
	// Do not offset pixel if the variation is incorrect.
	float finalOffset = correctVariation ? pixelOffset : 0.0;
	
	
	/* SUB-PIXEL SHIFTING */
	
	// Get full weighted average of the luma over the neighboring pixels.
	float lumaAverage = (2 * (lumaDU + lumaLR) + lumaCornersL + lumaCornersR) / 12;
	
	// Get ratio of the difference between average and center luma,
	// over the range of lumas in the neighboring pixels.
	float subPixelOffset = clamp(abs(lumaAverage - lumaCenter) / lumaRange, 0, 1);
	subPixelOffset = (-2.0 *  subPixelOffset + 3.0) * pow(subPixelOffset, 2);
	subPixelOffset = pow(subPixelOffset, 2) * 0.75;
	
	// Get the largest offset.
	finalOffset = max(finalOffset, subPixelOffset);
	
	// Get the final UV coordinates.
	vec2 finalUV = vTexCoord;
	finalUV[edgeDir] += finalOffset * stepLength;
	
	// Return the new color.
	return texture2D(uTexture, finalUV).rgb;
}

void main() {
	vec3 texColor = texture2D(uTexture, vTexCoord).rgb;
	vec3 pixelColor = fxaa(texColor);
    
	gl_FragColor = vec4(pixelColor, 1.0);
}]]></RmShaderCode></RmGLShader>
