#version 120

uniform sampler2D Diffuse;
uniform sampler2D Lightmap;
uniform sampler2D Normalmap;
uniform sampler2D Lightvec;
uniform sampler2D Depth;

uniform float contrast;
uniform float brightness;
uniform float saturation;

uniform float crosshairSize;
uniform vec4  crosshairColor;

uniform float bulgeIntensity;

varying vec2 texCoord;

const float CROSS_WIDTH  = 0.005;
const float CROSS_LENGTH = 0.03;

const vec2 CENTER      = vec2(0.5, 0.5);
const vec3 LUM_WEIGHTS = vec3(0.299, 0.587, 0.114);
const vec3 ZERO_CONTRAST = vec3(0.5);

/*
 * If value is between start and stop (inclusive) returns 1.0,
 * otherwise returns 0.0.
 */
float inRange(float start, float stop, float value) {
	return step(start, value) - step(stop, value);
}

// Convert from [0 - 1] to [-1 - 1].
vec3 unpack(vec3 value) {
	return (value - 0.5) * 2;
}

float getLuminance(vec3 color) {
	return dot(color, LUM_WEIGHTS);
}

vec2 lensBulge() {
	vec2 tex = texCoord - CENTER;
	float dist = length(tex);
	
	tex = CENTER + tex * pow(dist, bulgeIntensity);
	
	return tex;
}

vec3 lighting(vec2 texCoords) {
	// Unpack vectors.
	vec3 N = unpack(texture2D(Normalmap, texCoords).xyz);
	vec3 L = unpack(texture2D(Lightvec,  texCoords).xyz);
	
	// Read textures.
	vec3 lmapCol   = texture2D(Lightmap, texCoords).xyz;
	vec4 diffCol   = texture2D(Diffuse,  texCoords);
	vec4 depthPass = texture2D(Depth,    texCoords);
	
	float diffuse = dot(N, L);
    
    // First modify diffuse color, and then add lighting.
	vec3 result = diffCol.xyz * diffuse * diffCol.w * depthPass.z;
	result += lmapCol * depthPass.x;
	
	return result;
}

vec3 setContrast(vec3 color, float value) {
	return mix(ZERO_CONTRAST, color, value);
}

vec3 setBrightness(vec3 color, float value) {
	return color * value;
}

vec3 saturate(vec3 color, float value) {
	return mix(vec3(getLuminance(color)), color, value);
}

vec3 crosshair() {
	float halfWidth  = (CROSS_WIDTH  / 2.0) * crosshairSize;
	float halfLength = (CROSS_LENGTH / 2.0) * crosshairSize;
	
	float horzBar =
		inRange(CENTER.x - halfLength, CENTER.x + halfLength, texCoord.x) *
		inRange(CENTER.y - halfWidth,  CENTER.y + halfWidth,  texCoord.y);

	float vertBar =
		inRange(CENTER.x - halfWidth,  CENTER.x + halfWidth,  texCoord.x) *
		inRange(CENTER.y - halfLength, CENTER.y + halfLength, texCoord.y);
	
	float crosshair = clamp(horzBar + vertBar, 0.0, 1.0);
	
	return mix(vec3(0), crosshairColor.xyz, crosshair);
}

void main(void) {
	vec2 tex = lensBulge();
	
	vec3 pixelColor;
	
	pixelColor = lighting(tex);
	pixelColor = setContrast  (pixelColor, contrast  );
	pixelColor = setBrightness(pixelColor, brightness);
	pixelColor = saturate     (pixelColor, saturation);
	
	pixelColor += crosshair();
 
	gl_FragColor = vec4(pixelColor, 1.0);
}]]></RmShaderCode></RmGLShader>
