#version 120

uniform sampler2D uModelTex;
uniform sampler2D uEdgeTex;

varying vec2 vTexCoord;

void main() {
	vec3 modelColor = texture2D(uModelTex, vTexCoord).rgb;
	vec3 edgeColor  = texture2D(uEdgeTex,  vTexCoord).rgb;
	
	//  edgeColor *= step(1.0, dot(edgeColor, vec3(1)));
	edgeColor = mix(vec3(0), modelColor, dot(edgeColor, vec3(1)));
	
	vec3 pixelColor = (modelColor - edgeColor);
	
	gl_FragColor = vec4(pixelColor, 1.0);
}]]></RmShaderCode></RmGLShader>
