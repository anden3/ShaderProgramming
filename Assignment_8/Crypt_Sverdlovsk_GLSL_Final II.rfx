uniform sampler2D Texture;
uniform float LmapBlurArea;
uniform float glowMultiplier;

varying vec2 texCoord;

#define KERNEL_SIZE 15
vec2 kernel[KERNEL_SIZE];

void createKernel() {
	kernel[0]  = vec2(-7, 0.0017);
	kernel[1]  = vec2(-6, 0.0058);
	kernel[2]  = vec2(-5, 0.0163);
	kernel[3]  = vec2(-4, 0.0383);
	kernel[4]  = vec2(-3, 0.0742);
	kernel[5]  = vec2(-2, 0.1190);
	kernel[6]  = vec2(-1, 0.1580);
	kernel[7]  = vec2( 0, 0.1736);
	kernel[8]  = vec2( 1, 0.1580);
	kernel[9]  = vec2( 2, 0.1190);
	kernel[10] = vec2( 3, 0.0742);
	kernel[11] = vec2( 4, 0.0383);
	kernel[12] = vec2( 5, 0.0163);
	kernel[13] = vec2( 6, 0.0058);
	kernel[14] = vec2( 7, 0.0017);
}

vec3 glow(float area, float intensity) {
	vec3 result = vec3(0);

	for (int i = 0; i < KERNEL_SIZE; i++) {
		vec3 color = texture2D(
			Texture, texCoord + vec2(kernel[i].x * area, 0.0)
		).xyz;
		
		result += color * kernel[i].y * intensity;
	}
	
	return result;
}

void main(void) {
	createKernel();

	vec3 color = texture2D(Texture, texCoord).xyz;
	color += glow(LmapBlurArea, glowMultiplier);
	
	gl_FragColor = vec4(color, 1.0);
}]]></RmShaderCode></RmGLShader>
