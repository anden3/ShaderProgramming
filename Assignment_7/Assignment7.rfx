    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 120

uniform sampler2D Texture;

uniform float blurArea;

varying vec2 texCoord;

#define KERNEL_SIZE 11
vec2 kernel[KERNEL_SIZE];

void createKernel() {
	kernel[0]  = vec2(-5, 0.0250);
	kernel[1]  = vec2(-4, 0.0486);
	kernel[2]  = vec2(-3, 0.0816);
	kernel[3]  = vec2(-2, 0.1181);
	kernel[4]  = vec2(-1, 0.1474);
	kernel[5]  = vec2( 0, 0.1587);
	kernel[6]  = vec2( 1, 0.1474);
	kernel[7]  = vec2( 2, 0.1181);
	kernel[8]  = vec2( 3, 0.0816);
	kernel[9]  = vec2( 4, 0.0486);
	kernel[10] = vec2( 5, 0.0250);
}

void blurX(out vec3 color, float area) {
	vec3 result = vec3(0);

	for (int i = 0; i < KERNEL_SIZE; i++) {
  		vec3 color = texture2D(
			Texture, texCoord + vec2(kernel[i].x, 0) * area
		).rgb;
  	
  		result += color * kernel[i].y;
	}
	
	color = result;
}

void main() {
	createKernel();
	
	vec3 pixelColor;
	blurX(pixelColor, blurArea);
	 
	gl_FragColor = vec4(pixelColor, 1.0);
}]]></RmShaderCode></RmGLShader>
