#define KERNEL_SIZE 11
vec2 kernel[KERNEL_SIZE];

void createKernel() {
	kernel[0]  = vec2(-5, 0.0250);
	kernel[1]  = vec2(-4, 0.0486);
	kernel[2]  = vec2(-3, 0.0816);
	kernel[3]  = vec2(-2, 0.1181);
	kernel[4]  = vec2(-1, 0.1474);
	kernel[5]  = vec2( 0, 0.1587);
	kernel[6]  = vec2( 1, 0.1474);
	kernel[7]  = vec2( 2, 0.1181);
	kernel[8]  = vec2( 3, 0.0816);
	kernel[9]  = vec2( 4, 0.0486);
	kernel[10] = vec2( 5, 0.0250);
}

void blurY(out vec3 color, float area) {
	vec3 result = vec3(0);

	for (int i = 0; i < KERNEL_SIZE; i++) {
  		vec3 color = texture2D(
			Pass1, texCoord + vec2(0, kernel[i].x) * area
		).rgb;
  	
  		result += color * kernel[i].y;
	}
	
	color = result;
}

void colorize(inout vec3 color, float intensity) {
	color += vec3(texCoord * intensity, 0);
}

void vignette(inout vec3 color, float intensity, float shape, float vigStep) {
	vec2 tex = pow(abs(texCoord - CENTER) * intensity, vec2(shape));

	float dist = smoothstep(0.0, vigStep, ONE_MINUS(length(tex)));
	dist *= ONE_MINUS(distance(CENTER, texCoord));
	
	color *= dist;
}

void main() {
	createKernel();
	
	vec3 pixelColor;
	blurY(pixelColor, blurArea);
	colorize(pixelColor, colorizeIntensity);
	vignette(pixelColor, vignetteIntensity, vignetteShape, vignetteStep);
	
	gl_FragColor = vec4(pixelColor, 1.0);
}]]></RmShaderCode></RmGLShader>
