    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 120

uniform vec4 vViewPosition;

varying vec3 vNormal;
varying vec3 vTangent;
varying vec3 vBinormal;

varying vec2 vTexCoord;

attribute vec3 rm_Binormal;
attribute vec3 rm_Tangent;

void main(void) {
	vec4 vertex = gl_ModelViewProjectionMatrix * vec4(gl_Vertex.xyz + vViewPosition.xyz, 1);
	gl_Position = vertex;

	vNormal   = gl_Normal;
	vTangent  = rm_Binormal;
	vBinormal = rm_Tangent;
   
	vTexCoord = vec2(gl_MultiTexCoord0.x, -gl_MultiTexCoord0.y);
}
]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 120

varying vec2 vTexCoord;

varying vec3 vNormal;
varying vec3 vTangent;
varying vec3 vBinormal;

uniform sampler2D NormalMap;

void main(void) {
	vec3 B = normalize(vBinormal);
	vec3 N = normalize(vNormal);
	vec3 T = normalize(vTangent);
	
	vec3 curNormal = (texture2D(NormalMap, vTexCoord).xyz * 2) - 1.0;
	
	N = normalize((T * curNormal.x) + (B * curNormal.y) + (N * curNormal.z));
	
	gl_FragColor = vec4((N + 1) * 0.5, 1);
}]]></RmShaderCode></RmGLShader>
