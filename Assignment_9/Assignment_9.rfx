#version 120

uniform vec4 uToonMin;
uniform vec4 uToonMax;
uniform int  uToonSteps;

varying vec3 vNormal;
varying vec3 vEyeDir;
varying vec3 vLightDir;

vec3 discreteMix(vec3 start, vec3 end, int steps, float value, float maxValue = 1.0) {
	float stepSize = maxValue / steps;
	vec3 vectorStep = (end - start) / steps;
	
	float currentStep = clamp(floor(value / stepSize), 0, steps - 1);
	return start + (vectorStep * currentStep);
}

void main() {
	float intensity = dot(vLightDir, normalize(vNormal));
	vec3 pixelColor = discreteMix(uToonMin.rgb, uToonMax.rgb, uToonSteps, intensity);
    
	gl_FragColor = vec4(pixelColor, 1.0);
}]]></RmShaderCode></RmGLShader>
